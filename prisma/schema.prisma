// Prisma schema for DetailQuote waitlist

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// Waitlist signup entries
model WaitlistEntry {
  id        String   @id @default(cuid())
  email     String   @unique
  source    String   @default("landing_page")
  createdAt DateTime @default(now())
}
