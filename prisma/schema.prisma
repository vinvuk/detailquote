// Prisma schema for DetailQuote waitlist

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

/// Waitlist signup entries
model WaitlistEntry {
  id        String   @id @default(cuid())
  email     String   @unique
  source    String   @default("landing_page")
  createdAt DateTime @default(now())
}
